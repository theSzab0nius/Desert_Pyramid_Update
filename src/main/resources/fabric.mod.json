		"main": [
			"net.mossclock.desert_pyramid_update.Desert_pyramid_update"
		],
		"fabric-datagen": [
			"net.mossclock.desert_pyramid_update.Desert_pyramid_updateDataGenerator"
		],
      "client": [
        "net.mossclock.desert_pyramid_update.client.DesertPyramidUpdateClient"
      ]
